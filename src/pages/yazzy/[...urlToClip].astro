---
import BlogPostLayout from "../../layouts/BlogPostLayout.astro";
export const prerender = false;
interface Frontmatter {
	title: string;
	description: string;
	pubDate: number;
	tags: string[];
}
const { urlToClip } = Astro.params;
const fakeFrontmatter = {
	title: `${urlToClip} | yazzy`,
	description: "lorem ipsum sit dolor amet",
	pubDate: new Date(),
	tags: [],
};
---

<BlogPostLayout title={fakeFrontmatter.title} frontmatter={fakeFrontmatter}>
	{urlToClip}
</BlogPostLayout>
